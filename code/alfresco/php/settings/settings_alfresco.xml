<?xml version="1.0" encoding="UTF-8"?>
<application name="alfresco">
	<settings>
		<category name="credentials">
			<setting name="username" field="text" default="">
				<validations>
					<validation rule="required" message="Gelieve uw gebruikernaam in te vullen" />
				</validations>
			</setting>
			<setting name="password" field="text" default="">
				<validations>
					<validation rule="required" message="Gelieve uw wachtwoord in te vullen" />
				</validations>
			</setting>
		</category>
	</settings>
</application>